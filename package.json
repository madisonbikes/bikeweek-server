{
  "name": "bikeweek-backend",
  "version": "1.1.2",
  "description": "Imports events from Gravity Forms form data into Sched",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "compile": "tsc --noEmit",
    "dist": "tsc && npm run genversion",
    "dev": "ts-node-dev --inspect -- src/index.ts",
    "pm2-start": "npx --yes pm2 startOrRestart ecosystem.config.js --update-env",
    "pm2-stop": "npx --yes pm2 stop ecosystem.config.js",
    "lint": "eslint . --max-warnings=0",
    "check-package-updates": "npx npm-check-updates",
    "test": "NODE_ENV=test npx jest --maxWorkers=50%",
    "test-watch": "NODE_ENV=test npx jest --maxWorkers=25% --watch",
    "coverage": "NODE_ENV=test npx jest --maxWorkers=50% --coverage=true",
    "prepare": "husky",
    "genversion": "genversion -f -u dist/src/backend-version.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madisonbikes/bikeweek-backend.git"
  },
  "author": "Ben Sandee",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.x"
  },
  "bugs": {
    "url": "https://github.com/madisonbikes/bikeweek-backend/issues"
  },
  "homepage": "https://github.com/madisonbikes/bikeweek-backend#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "build-url": "^6.0.1",
    "connect-redis": "^7.1.1",
    "connection-string": "^4.4.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "google-auth-library": "^9.10.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "mongodb": "^6.8.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pino": "^9.1.0",
    "pino-pretty": "^11.2.1",
    "redis": "^4.6.14",
    "set-interval-async": "^3.0.3",
    "superagent": "^9.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.13.0",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/set-interval-async": "^1.0.3",
    "@types/superagent": "^8.1.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-autofix": "^2.0.0",
    "eslint-plugin-promise": "^6.2.0",
    "genversion": "^3.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.4.0",
    "prettier": "3.3.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.11.0"
  }
}